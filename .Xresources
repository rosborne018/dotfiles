!#include "/home/nir/.colors/colored"
!#include "/home/nir/.colors/tron"
!#include "/home/nir/.colors/eup2"
!#include "/home/nir/.colors/nancy"
!#include "/home/nir/.colors/hybrid"

! Appearance
URxvt.font:           xft:Inconsolata\ For\ Powerline:size=10:antialias=true
URxvt.imFont:         xft:Inconsolata\ For\ Powerline:size=10:antialias=true
URxvt.boldFont:       xft:Inconsolata\ For\ Powerline:size=10:antialias=true
URxvt.italicFont:     xft:Inconsolata\ For\ Powerline:size=10:antialias=true
URxvt.boldItalicFont: xft:Inconsolata\ For\ Powerline:size=10:antialias=true


!xft:Terminus (TTF) for Powerline:MediumForPowerline:size=10:antialias=true,
!xft:Terminus (TTF) for Powerline:MediumForPowerline:size=10:antialias=true,
!xft:Terminus (TTF) for Powerline:MediumForPowerline:size=10:antialias=true,URxvt.intensityStyles: false
!xft:Terminus (TTF) for Powerline:MediumForPowerline:size=10:antialias=true,URxvt.iconFile:       /usr/share/icons/Faenza/apps/scalable/terminal.svg
xft:Terminus (TTF) for Powerline:MediumForPowerline:size=10:antialias=true,URxvt.boldMode:       false
URxvt.skipBuiltinGlyphs: true
URxvt.loginShell: true
URxvt*title: urxvt
URxvt*termName: rxvt-unicode-256color
URxvt*urgentOnBell: true
URxvt.letterSpace:	-1

URxvt.borderColor:          #050505
URxvt.borderless:           false
URxvt.highlightColor:       #a761c2
URxvt.highlightTextColor:   #151515

URxvt.cursorColor:          #dedede
URxvt.cursorColor2:         #151515
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false

URxvt.depth: 24
URxvt.internalBorder: 1

! Bold/Italic/Underline Styles
URxvt.colorBD:              #ffffff
URxvt.colorIT:              #a761c2
URxvt.colorUL:              #6d9cbe
URxvt.underlineColor:       #333333

! ScrollBar
URxvt.scrollBar:            false
URxvt.scrollBar_right:      false
URxvt.scrollstyle:          plain
URxvt.scrollColor:          #ffffff
URxvt.jumpScroll:           false

URxvt.scrollWithBuffer:     true
URxvt.scrollTtyKeypress:    true
URxvt.scrollTtyOutput:      false

URxvt.buffered:             true
URxvt.pointerBlank:         true
URxvt.underlineURLs:        true
URxvt.saveLines:            2048

! Transparency
URxvt.transparent:          false

! URLs

URxvt.perl-ext:              default,url-select,matcher,clipboard,font-size
URxvt.keysym.M-u:            perl:url-select:select_next
URxvt.url-select.launcher:   /usr/bin/chromium-dev
URxvt.url-select.underline:  true
URxvt.keysym.C-Up:           perl:font-size:increase 
URxvt.keysym.C-Down:         perl:font-size:decrease 
URxvt.keysym.C-S-Up:         perl:font-size:incglobal 
URxvt.keysym.C-S-Down:       perl:font-size:decglobal 
URxvt.iso14755:              false
URxvt.iso14755_52:           false
URxvt.keysym.M-c:            perl:clipboard:copy
URxvt.keysym.M-v:            perl:clipboard:paste

!-¬
